    t = 1;
    //cin >> t;
    //vector<ll> val((int)1e6+7,0);
    //prime.resize(1000005,true);
    //seive(1000005);
    
    for (int i = 1; i <= t; i++)
    {
        
        //cout << "Case #" << i << ": ";
        solve();
    }

    return 0;
}
