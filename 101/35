void mediumNum(int num[]){
    for(int i=0;i<3;i++){
        int max=*max_element(num,num+3);
        int min=*min_element(num,num+3);
        if (max!=num[i]&& min!=num[i]){
            cout<<num[i]<<endl;

        }
    }
}

void atilla () {
    char letters[] = {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p', 'q', 'r', 's',
                      't', 'u', 'v', 'w', 'x', 'y', 'z'};
    int si=sizeof (letters)/sizeof (letters[0]);


        int size;
        cin >> size;
        int const s = size;
        string word;
        cin >> word;
        int j;
        int i;
        int temp;
        int arr[s];
        int t;

        for ( i = 0; i < s; i++) {
            for ( j = 0; j < si; j++) {
                if (word[i] == letters[j] ) {
                    temp = j;
                }
            }
            t=temp + 1 ;
        }

        for(int k=0;k<s;k++) {
            arr[k] = t;
        }
        int max = *max_element(arr, arr + s);
        cout<<max<<endl;
    }

void atilla2(string word){
    int max1 = INT64_MIN;
    for(char x : word)
    {
        int temp = x - 96;
        max1 = max(max1, temp);
    }
    cout << max1 << endl;
}

