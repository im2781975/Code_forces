//I Wanna Be the Guy
/*
#include <iostream>

using namespace::std;

int remove_duplicate_elements(int arr[], int n)
{

    if (n==0 || n==1)
        return n;

    int temp[n];

    int j = 0;
    int i;
    for (i=0; i<n-1; i++)
    {
        if (arr[i] != arr[i+1])
        {
            temp[j++] = arr[i];
            temp[j++] = arr[n-1];
        }
    }
    
    for (i=0; i<j; i++)
            arr[i] = temp[i];

return j;
}

int main()
{
    int n,i,j;
    
    cin >> n;
    
    int a,b;
    
    cin >> a;
    int x[a];
    for (i=0; i<a; i++)
    {
        cin >> x[i];
    }
    
    remove_duplicate_elements(&x[a], a);
    
    cin >> b;
    int y[b];
    for (i=0; i<b; i++)
    {
        cin >> y[i];
    }
    
    remove_duplicate_elements(&y[b], b);
    
    int count=n;
    
    for (i=0; i<a; i++)
    {
        for (j=0; j<n; j++)
        {
            if (x[i]==j)
            {
                count--;
            }
        }
    }
    
    for (i=0; i<b; i++)
    {
        for (j=0; j<n; j++)
        {
            if (y[i]==j)
            {
                count--;
            }
        }
    }
    
    if (count==0)
    {
        cout << "I become the guy.";
    }
    else
        cout << "Oh, my keyboard!";

}

*/

