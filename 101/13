{
    
    int t,m;
    
    cin >> t;
    
    m=t;
    long long int ans[t];
    
    while (t--)
    {
        long long int n;
        
        cin >> n;
        
        long long int x=n/2;
        
        ans[t]=x;
        
    }
    
    while (m--)
    {
        cout << ans[m] << endl;
    }
    
    
    
}
