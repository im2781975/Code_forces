        i=i/10;
        
        z=i%10;
        
        if (w==x || w==y ||  w==z || x==y || x==z || y==z)
        {
            year[i]=i;
        }
    }
    
    long int count=0;
    long int n;
    
    cin >> n;
    
    for (i=1000; i<=9000; i++)
    {
        if (n==year[i])
        {
            count=year[i];
