int main(){
	long long t, n,c,l,r,ans;
	string s;
	long long arr[5][5];
	for(int i=0;i<5;i++){
		for(int j=0;j<5;j++){
			cin>>arr[i][j];
			if(arr[i][j]==1){
				r=i;
				c=j;
			}
		}
	
	}
	ans=abs(2-r)+abs(2-c);
	cout<<ans<<endl;
}
	
	
