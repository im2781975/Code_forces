}
int main()
{
  code();
    int cnt = 0; 
   tc
    {
       string s;
       cin>>s ;
      
        if(s[0] == '+')
        {
            ++cnt;
        }
              if(s[2] == '+')
        {
            cnt++;
        }
                if(s[0] == '-')
        {
            --cnt;
        }
              if(s[2] == '-')
        {
            cnt--;
        }
     }
     cout<<cnt;

}
