	{
		for (int c = 0; c < 5; c++)
		{
			cin >> arr[r][c];
			if (arr[r][c] == 1)
			{
				i = r;
				j = c;	
			}
		}
	}
	cout << abs(i - 2) + abs(j - 2) << endl;
